<div class='songs' id='songs'>
  {{#each this}}
  <div>
    <h2 id='songTitle'>{{title}}</h2>
    <ul id='songlist'>
	    <li id='artist'>{{artist}}</li>
	    <li id='album'>{{album}}</li>
    </ul>
    <button id='remove' type='button'>Remove</button>
    <button id='delete' type='button'>Delete</button>
  </div>
  {{/each}}
</div>  

